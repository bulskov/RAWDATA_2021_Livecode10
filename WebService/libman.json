{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [

    {
      "provider": "unpkg",
      "library": "requirejs@2.3.6",
      "destination": "wwwroot/js/lib/requirejs/",
      "files": [ "require.js" ]
    },
    {
      "provider": "unpkg",
      "library": "requirejs-text@2.0.16",
      "destination": "wwwroot/js/lib/requirejs/",
      "files": [ "text.js" ]
    },
    {
      "provider": "unpkg",
      "library": "jquery@3.6.0",
      "destination": "wwwroot/js/lib/jquery/",
      "files": [ "dist/jquery.js", "dist/jquery.min.js", "dist/jquery.slim.js" ]
    },
    {
      "provider": "unpkg",
      "library": "knockout@3.5.1",
      "destination": "wwwroot/js/lib/knockout/",
      "files": [ "build/output/knockout-latest.debug.js", "build/output/knockout-latest.js" ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/css/lib/bootstrap/",
      "files": [ "dist/css/bootstrap.css", "dist/css/bootstrap.min.css" ]
    }
  ]
}